@model IEnumerable<psiconexao.Models.Disponibilidade>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DataInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataFim)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HoraFim)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Psicologo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DataInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataFim)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraInicio)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HoraFim)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Psicologo.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DisponibilidadeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DisponibilidadeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DisponibilidadeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
